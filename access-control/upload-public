const cloudinary = require('cloudinary').v2


// type:upload  the default and it indicates the asset is public
// resource_type the default is image

cloudinary.uploader.upload('./banana.jpg', {
  public_id: 'banana',
  type: 'upload',
  resource_type: 'image'
})
  .then(result => {
    console.log(result.secure_url)
  })
  .catch(error => {
    console.log(error)
  })
